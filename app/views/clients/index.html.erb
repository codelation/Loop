<h2>Your Current Clients</h2>

<table>
  <% @clients.each do |client| %>
    <tr>
      <td><%= client.name %></td>
      <td><%= client.company_name %></td>
      <td><%= button_to "Edit", edit_client_path(client) %></td>
      <td><%= button_to "Delete", client_path(client), method: "delete" %></td>
    </tr>
  <% end %>
</table>
